{
  "name": "pti/pti-twig",
  "type": "typo3-cms-extension",
  "description": "Prototype Integration Twig integration",
  "authors": [
    {
      "name": "Hannes Lau",
      "email": "office@hanneslau.de"
    }
  ],
  "require": {
    "php": "^7.4",
    "pti/pti": "^3.0",
    "twig/twig": "^2.0",
    "typo3/cms-core": "^10.4"
  },
  "autoload": {
    "psr-4": {
      "PrototypeIntegration\\Twig\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "pti_twig"
    }
  },
  "replace": {
    "typo3-ter/pti-twig": "self.version"
  }
}
